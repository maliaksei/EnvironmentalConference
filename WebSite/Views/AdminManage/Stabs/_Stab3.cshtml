@using Trirand.Web.Mvc
@model Models.JqGridModels.ResearchAreasJqGridModel

@Html.Trirand().JQGrid(Model.OrdersGrid, "ResearchAreasJQGrid")

@Html.Trirand().JQDatePicker(
            new JQDatePicker
                {
                    DisplayMode = DatePickerDisplayMode.ControlEditor,
                    ShowOn = ShowOn.Both,
                    DateFormat = "yyyy/mm/dd",
                    MinDate = DateTime.Now.Date,
                    MaxDate = new DateTime(2100,12,1),
                    AutoSize = true
                }, "DatePicker")

